<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <!--
        Add Columns max_sessions, session_max_age to security_app
    -->
    <changeSet id="20160624103732-1" author="hookszhang">
        <addColumn tableName="security_app">
            <column name="max_sessions" type="int(3)" valueNumeric="2">
                <constraints nullable="false" />
            </column>
        </addColumn>
        <addColumn tableName="security_app">
            <column name="session_max_age" type="bigint" valueNumeric="604800000">
                <constraints nullable="false" />
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>
