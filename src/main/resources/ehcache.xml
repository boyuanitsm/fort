<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.boyuanitsm.fort.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.boyuanitsm.fort.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.boyuanitsm.fort.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.boyuanitsm.fort.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.boyuanitsm.fort.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.boyuanitsm.fort.domain.SecurityApp"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityResourceEntity"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityResourceEntity.authorities"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityNav"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityAuthority"
           timeToLiveSeconds="3600">
