    <cache name="com.boyuanitsm.fort.domain.SecurityRole"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityRole.users"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityRole.authorities"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityUser"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityUser.groups"
           timeToLiveSeconds="3600">
    </cache>
    <cache name="com.boyuanitsm.fort.domain.SecurityUser.roles"
